<!--Page Header-->
<div class="row" id="main" >
        <div class="col-sm-12 col-md-12 wellbg-dark text-white" id="content">
            <h1 class="text-center">Upcoming To Do's<i class="fa fa-fw fa-star" style="color: yellow"></i></h1>
        </div>
    </div>
    <hr>
    
    <!-- ↓↓↓  Main Content Goes Below ↓↓↓ -->
    
    <ul>
        <% todoArray.forEach(function(todo) { %>
            <li class="leads <%= todo.gtodoCompleted %>" data-lead="<%= todo.todoId %>" data-id="<%= todo.todoleadId %>"><a href="#"  ><%= todo.todoleadPosition %>, <%= todo.todoleadCompany %> >> <%= todo.todoTaskName %>: <%= todo.todoContent%></a> <button type="button" class="btn btn-default btn-xs">Mark as Complete</button> <button type="button" class="btn btn-default btn-xs">Delete</button></li>
        <% }); %>
    </ul>